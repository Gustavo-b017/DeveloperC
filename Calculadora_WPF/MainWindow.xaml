<Window x:Class="Calculadora_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Calculadora_WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid RenderTransformOrigin="0.607,0.494">
        <TextBox x:Name="testeSaida" HorizontalAlignment="Left" Margin="560,373,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="textoSaida_TextChanged"/>
        <Button x:Name="processor" Content="Somar" HorizontalAlignment="Left" Margin="477,373,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <TextBox x:Name="CampoEntrada" HorizontalAlignment="Left" Margin="69,102,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="160" Height="51" TextChanged="CampoEntrada_TextChanged"/>
        <Button x:Name="BtnBuffer" Content="Enter" HorizontalAlignment="Left" Margin="234,102,0,0" VerticalAlignment="Top" Height="51" Width="100" Click="BtnBuffer_Click_1" />
        <TextBox x:Name="conteudo" HorizontalAlignment="Left" Margin="477,85,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="203" Height="223"/>
        <Button x:Name="clear" Content="Clear" HorizontalAlignment="Left" Margin="284,260,0,0" VerticalAlignment="Top" Height="33" Width="61" Click="clear_Click"/>

    </Grid>
</Window>
